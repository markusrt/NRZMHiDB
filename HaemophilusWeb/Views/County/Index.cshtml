@model IEnumerable<HaemophilusWeb.Models.County>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CountyNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ValidSince)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CountyNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ValidSince)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CountyId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CountyId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CountyId })
        </td>
    </tr>
}

</table>
