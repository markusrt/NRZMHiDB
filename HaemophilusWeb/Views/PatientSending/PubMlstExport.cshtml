@using HaemophilusWeb.Views.Utils
@model HaemophilusWeb.ViewModels.FromToQueryWithAdjustment

@{
    ViewBag.Title = "Export der PubMLST Daten (Haemophilus)";
}

<h3>Entnahmedatum</h3>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-horizontal">
    @Html.ValidationSummary(true)




    <div class="form-group">
        @Html.DateEditorFor(m => m.From)
        @Html.DateEditorFor(m => m.To)
    </div>
    <div class="form-group">
        @Html.EnumRadioEditorFor(m => m.Unadjusted)
        <small class="form-text text-muted col-sm-offset-2">Unbereinigt: Auch Daten aus dem Ausland, alle Entnahmeorte, doppelte Einsendungen enthalten.</small>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" name="primary-submit" value="Exportieren nach Excel" class="btn btn-primary" />

        </div>
    </div>
</div>
}

