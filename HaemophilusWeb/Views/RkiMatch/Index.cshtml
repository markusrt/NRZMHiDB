@model dynamic

<h2>RKI Matching Daten</h2>

@using (Html.BeginForm("Index", "RkiMatch", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-sm-3">
                @Html.Label("CSV Datei")
            </div>
            <div class="col-sm-5">
                <div class="input-group">
                    @Html.TextBox("file", "", new {type = "file"})
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-12">
                <input type="submit" value="Upload" class="btn btn-primary" />
            </div>
            <div class="col-md-12">
                @ViewBag.Message
            </div>
        </div>
    </div>
}
